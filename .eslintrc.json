{
    "extends": "semistandard",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "globals": {
        "BigInt": true
    },
    "rules": {
        "prefer-const": ["error", {
            "destructuring": "any",
            "ignoreReadBeforeAssign": false
        }],
        "quotes": ["error", "single"],
        "no-unused-vars": ["warn", {
            "vars": "all",
            "args": "after-used",
            "argsIgnorePattern": "Apollo|msg|args|guildEntry|addCD|Currency"
        }]
    }



}